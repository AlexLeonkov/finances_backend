generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operation {
  id        Int      @id @default(autoincrement())
  name      String
  team      String?
  date      DateTime
  revenue   Float
  profit    Float
  createdAt DateTime @default(now())
}
