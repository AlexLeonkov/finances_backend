generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Operation {
  id        Int      @id @default(autoincrement())
  invoiceNumber      String?
  team      String?
  members   String?
  date      DateTime
  revenue   Float
  materialCost Float?
  fuelCost Float?
  isPaid Boolean?
  profit    Float
  createdAt DateTime @default(now())
}
